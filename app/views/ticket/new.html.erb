<%= form_for :ticket, url: ticket_new_path do |f| %>
    <p>
      <%= f.label :title %><br>
      <%= f.text_field :title %>
    </p>

    <p>
      <%= f.label :description %><br>
      <%= f.text_area :description %>
    </p>

    <p>
      <%= f.label :added_by %><br>
      <%= f.text_area :added_by %>
    </p>

    <p>
      <%= f.submit %>
    </p>
<% end %>